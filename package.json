{
  "name": "rest-api-typescript",
  "version": "1.0.0",
  "description": "A REST API boilerplate written in TypeScript with Express, Jest, Supertest, ESLint, Prettier, ESLint, Nodemon, Debug, Winston, Morgan, Helmet, Cors, Dotenv, Zod, and more.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc src/**/*.ts --write",
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node build/index.js",
    "dev": "npx nodemon"
  },
  "keywords": [
    "typescript",
    "node",
    "express",
    "rest",
    "api",
    "rest-api",
    "rest-api-typescript",
    "jest",
    "ts-jest",
    "supertest",
    "eslint",
    "prettier",
    "tslint",
    "nodemon",
    "helmet",
    "cors",
    "morgan",
    "winston",
    "debug",
    "dotenv",
    "zod"
  ],
  "author": "Nitin Sharma",
  "license": "ISC",
  "dependencies": {
    "@types/morgan": "^1.9.9",
    "@types/winston": "^2.4.4",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "^5.3.3"
  }
}
